{
  "name": "scraper-api-server",
  "version": "1.0.0",
  "description": "实时 Temu 爆款爬虫 API 服务器",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "test:scraper": "tsx test-scraper.ts",
    "test:visible": "HEADLESS=false DEBUG=true tsx test-scraper.ts",
    "test:human": "tsx test-human-behavior.ts",
    "test:diagnose": "tsx test-diagnose.ts",
    "test:stealth": "tsx test-stealth.ts",
    "test:real-chrome": "tsx test-real-chrome.ts",
    "test:real-chrome:cookies": "tsx test-real-chrome-with-cookies.ts",
    "start:visible": "HEADLESS=false DEBUG=true tsx src/index.ts",
    "install:stealth": "npm install puppeteer-extra puppeteer-extra-plugin-stealth"
  },
  "keywords": [
    "scraper",
    "temu",
    "api",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "playwright": "^1.40.0",
    "puppeteer": "^21.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "tsx": "^4.7.0"
  }
}
